		{"id":"a39aba164a8d645b","type":"group","x":-3540,"y":5970,"width":972,"height":760,"label":"0 HP"},
		{"id":"b107e6a10c80e67f","x":-3340,"y":5093,"width":906,"height":767,"type":"group","label":"Damage"},
		{"id":"be9e9cb96268a4ad","x":-6560,"y":4820,"width":1180,"height":580,"type":"group","label":"Hero Points"},
		{"type":"group","id":"99f1a9a2af2efa21","x":-5720,"y":6340,"width":678,"height":749,"label":"Counteract Checks"},
		{"id":"54ec69dd11c808dc","x":-5460,"y":3704,"width":855,"height":588,"type":"group","label":"Special Senses"},
		{"id":"33eba5ce94029e9f","x":-7487,"y":4466,"width":697,"height":714,"type":"group","label":"Item Breaking"},
